<Window x:Class="FB.SuperSearcher.Frontend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FB.SuperSearcher.Frontend"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="SearchTextbox" HorizontalAlignment="Left" Margin="30,25,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="20"/>
        <Button Content="søg" HorizontalAlignment="Left" Margin="152,25,0,0" VerticalAlignment="Top" Width="41" Click="PerformSearch"/>
        <ListView x:Name="SearchResultListView" Margin="0,72,0,0">
            <ListView.View >
                <GridView>
                    <GridViewColumn Header="Is Web Result" DisplayMemberBinding="{Binding IsWebResult}"/>
                    <GridViewColumn Header="Path" DisplayMemberBinding="{Binding Path}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
